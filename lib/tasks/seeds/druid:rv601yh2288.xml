<foxml:datastream ID="workflowDefinition" STATE="A" CONTROL_GROUP="X" VERSIONABLE="true">
<foxml:datastreamVersion ID="workflowDefinition.0" LABEL="Workflow Definition" CREATED="2011-10-26T23:23:58.100Z" MIMETYPE="text/xml" SIZE="1357">
<foxml:xmlContent>
<workflow-def id="preservationIngestWF" repository="dor">
          <process name="start-ingest" sequence="1">
            <label>Initiate deposit</label>
          </process>
          <process name="register-sdr" sequence="2">
            <label>Resgister object in Sedora</label>
            <prereq>start-ingest</prereq>
          </process>
          <process name="transfer-object" sequence="3">
            <label>Transfer contents from DOR</label>
            <prereq>register-sdr</prereq>
          </process>
          <process name="validate-bag" sequence="4">
            <label>Verify the bagit bag structure</label>
            <prereq>transfer-object</prereq>
          </process>
          <process name="populate-metadata" sequence="5">
            <label>Pull selected metadata files into Sedora</label>
            <prereq>validate-bag</prereq>
          </process>
          <process name="verify-agreement" sequence="6">
            <label>Verify there is an ingested agreement object for object</label>
            <prereq>validate-bag</prereq>
          </process>
          <process name="complete-deposit" sequence="7">
            <label>Done; transfer control to full ingest process</label>
            <prereq>populate-metadata</prereq>
            <prereq>verify-agreement</prereq>
          </process>
        </workflow-def>
</foxml:xmlContent>
</foxml:datastreamVersion>
<foxml:datastreamVersion ID="workflowDefinition.1" LABEL="Workflow Definition" CREATED="2018-02-23T23:54:00.619Z" MIMETYPE="text/xml" SIZE="1282">
<foxml:xmlContent>
<workflow-def id="preservationIngestWF" repository="sdr">
  <process name="start-ingest" sequence="1" status="completed">
    <label>Initiate ingest workflow</label>
  </process>
  <process name="transfer-object" sequence="2">
    <label>Pull new or versioned object from DOR</label>
    <prereq>start-ingest</prereq>
  </process>
  <process name="validate-bag" sequence="3">
    <label>Verify the bagit bag structure</label>
    <prereq>transfer-object</prereq>
  </process>
  <process name="verify-apo" sequence="4">
    <label>Check that governing APO has been ingested</label>
    <prereq>validate-bag</prereq>
  </process>
  <process name="manage-moab" sequence="5">
    <label>Create/update Moab object</label>
    <prereq>verify-apo</prereq>
  </process>
  <process name="manage-catalog" sequence="5">
    <label>Create/update Preservation Catalog entries</label>
    <prereq>manage-moab</prereq>
  </process>
  <process name="initiate-replication" sequence="6" status="completed">
    <label>Initiate replication of archive objects</label>
    <prereq>manage-catalog</prereq>
  </process>
  <process name="complete-deposit" sequence="8">
    <label>Done; transfer control tback to accessioning</label>
    <prereq>initiate-replication</prereq>
  </process>
</workflow-def>
</foxml:xmlContent>
</foxml:datastreamVersion>
<foxml:datastreamVersion ID="workflowDefinition.2" LABEL="Workflow Definition" CREATED="2018-03-14T23:42:12.981Z" MIMETYPE="text/xml" SIZE="1170">
